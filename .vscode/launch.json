{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "name": "ChessMint debugger",
            "port": 9992,
            "url": "https://www.chess.com/*",
            "webRoot": "${workspaceFolder}/dist",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/dist/js/*.js"
            ],
        }
    ]
}